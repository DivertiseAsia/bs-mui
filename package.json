{
  "name": "@divertise-asia/bs-mui",
  "version": "1.12.2",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w -ws _",
    "clean": "bsb -clean-world",
    "server": "moduleserve ./ --port 8000",
    "setup-test": "npm install bs-platform bs-react-testing-library @glennsl/bs-jest bs-webapi bs-jest-dom --legacy-peer-deps",
    "test": "jest --testMatch='**/__tests__/*test.bs.js'",
    "ci-test": "npm run clean && npm run build && npm test -- --coverage --updateSnapshot",
    "build-doc": "bsdoc build . && bsdoc support-files"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "Divertise Asia",
  "license": "MIT",
  "repository": {
    "type": "https",
    "url": "https://github.com/DivertiseAsia/bs-mui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.10.8",
    "@date-io/moment": "^2.10.8",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "date-fns": "^2.16.1",
    "moment": "^2.29.1",
    "nan": "^2.14.2",
    "popper.js": "^1.16.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.6.0",
    "bs-jest-dom": "^4.1.1",
    "bs-platform": "^8.4.2",
    "bs-react-testing-library": "^0.8.0",
    "bs-webapi": "^0.19.1",
    "moduleserve": "github:marijnh/moduleserve#7db414e",
    "reason-react": "^0.9.1"
  }
}
