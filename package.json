{
  "name": "@divertise_asia/bs-mui",
  "version": "1.4.3",
  "scripts": {
    "build": "bsb -make-world",
    "start": "bsb -make-world -w -ws _",
    "clean": "bsb -clean-world",
    "server": "moduleserve ./ --port 8000",
    "setup-test": "npm install bs-platform bs-react-testing-library @glennsl/bs-jest bs-webapi bs-jest-dom",
    "test": "jest --testMatch='**/__tests__/*test.bs.js'",
    "ci-test": "npm test -- --coverage --updateSnapshot",
    "build-doc": "bsdoc build . && bsdoc support-files"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "Divertise Asia",
  "license": "MIT",
  "repository": {
    "type": "https",
    "url": "https://github.com/DivertiseAsia/bs-mui.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@date-io/moment": "^1.3.13",
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.50",
    "@material-ui/pickers": "^3.2.10",
    "moment": "^2.24.0",
    "nan": "^2.14.1",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reason-react": "^0.7.1"
  },
  "devDependencies": {
    "@glennsl/bs-jest": "^0.5.1",
    "bs-jest-dom": "^2.0.2",
    "bs-platform": "^7.3.1",
    "bs-react-testing-library": "^0.6.0",
    "bs-webapi": "^0.15.9",
    "bsdoc": "^6.0.2-alpha",
    "moduleserve": "github:marijnh/moduleserve#7db414e"
  }
}
